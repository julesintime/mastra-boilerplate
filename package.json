{
  "name": "create-mastra-agent-framework",
  "version": "1.0.0",
  "description": "Bootstrap a comprehensive Mastra AI agent framework with RAG, security, and workflow capabilities",
  "main": "index.js",
  "bin": {
    "create-mastra-agent-framework": "./create-mastra-agent-framework"
  },
  "keywords": ["mastra", "ai", "agents", "rag", "framework", "template", "boilerplate"],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/create-mastra-agent-framework.git"
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "dev": "mastra dev",
    "build": "mastra build",
    "start": "mastra start",
    "eval": "tsx scripts/run-evaluations.ts",
    "eval:ci": "EVAL_CI_ENABLED=true tsx scripts/run-evaluations.ts",
    "eval:strict": "EVAL_CI_ENABLED=true EVAL_CI_FAIL_ON_THRESHOLD=true tsx scripts/run-evaluations.ts",
    "rag:init": "tsx scripts/initialize-rag.ts",
    "rag:reset": "tsx scripts/initialize-rag.ts --reset"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.23",
    "@mastra/core": "^0.11.1",
    "@mastra/evals": "^0.10.7",
    "@mastra/libsql": "^0.11.2",
    "@mastra/loggers": "^0.10.4",
    "@mastra/mcp": "^0.10.7",
    "@mastra/memory": "^0.11.5",
    "ai": "^4.0.21",
    "bcrypt": "^5.1.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "validator": "^13.15.15",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.2",
    "@vitest/coverage-v8": "^3.2.4",
    "mastra": "^0.10.15",
    "supertest": "^7.1.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
